{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "whatsapp",
  "rules": {
    "conversation": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "message"
      }
    },
    "message": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "content"
        }
      ]
    },
    "header": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "FIELD",
              "name": "timestamp",
              "content": {
                "type": "PATTERN",
                "value": "[ \\u202f\\d\\/\\-.,:]*(AM|PM)?"
              }
            },
            {
              "type": "STRING",
              "value": "]"
            },
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "FIELD",
              "name": "from",
              "content": {
                "type": "PATTERN",
                "value": "[^\\n:]{1,25}"
              }
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "PATTERN",
              "value": "\\s+"
            }
          ]
        }
      }
    },
    "content": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": ".*\\n"
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}