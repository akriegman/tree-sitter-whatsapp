{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "whatsapp",
      "camelcase": "WhatsApp",
      "title": "WhatsApp",
      "scope": "source.whatsapp",
      "file-types": [
        "txt"
      ],
      "injection-regex": "^whatsapp$",
      "class-name": "TreeSitterWhatsapp"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Grammar for WhatsApp conversation transcripts",
    "authors": [
      {
        "name": "Aaron Kriegman",
        "email": "aaronkplus2@gmail.com",
        "url": "https://aaah.run/"
      }
    ],
    "links": {
      "repository": "https://github.com/akriegman/tree-sitter-whatsapp"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}